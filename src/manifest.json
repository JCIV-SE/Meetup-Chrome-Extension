{
  "name": "Meetup Chrome Extension",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon-v1-34x34.png"
  },
  "icons": {
    "16": "img/icon-v1-16x16.png",
    "34": "img/icon-v1-34x34",
    "48": "img/icon-v1-48x48.png",
    "128": "img/icon-v1-128x128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
